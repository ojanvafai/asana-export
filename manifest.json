{
  "name": "Export Asana",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "downloads",
    // TODO: add for all the off-asana storage integrations, e.g.
    // Google Drive, Box, and OneDrive.
    "https://www.dropbox.com/",
    "https://app.asana.com/"
  ],
  "background": {
    "scripts": ["jszip.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "incognito.png",
    "default_title": "Export"
  }
}
